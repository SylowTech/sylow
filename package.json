{
  "name": "sylow",
  "version": "1.0.0",
  "description": "An efficient, simple, encrypted personal data store and sharing system.",
  "author": "Jacob Cook <jacob@peakwinter.net>",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=4.8.0",
    "npm": ">=2.15.11",
    "yarn": ">=0.20.3"
  },
  "scripts": {
    "start": "gulp serve",
    "start:debug": "cross-env DEBUG=sylow:* yarn start",
    "build": "gulp",
    "lint": "esw *.js admin server cli config tests --color",
    "lint:watch": "yarn lint -- --watch",
    "prepush": "yarn lint && yarn test",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors --compilers js:babel-core/register tests --recursive",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors --compilers js:babel-core/register tests --recursive",
    "report-coverage": "coveralls < ./coverage/lcov.info && codeclimate-test-reporter < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peakwinter/sylow.git"
  },
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/peakwinter/sylow/issues"
  },
  "homepage": "https://github.com/peakwinter/sylow#readme",
  "dependencies": {
    "ajv": "5.0.1",
    "axios": "^0.16.1",
    "babel-plugin-array-includes": "2.0.3",
    "bluebird": "3.4.6",
    "body-parser": "1.15.2",
    "commander": "2.9.0",
    "compression": "1.6.2",
    "connect-ensure-login": "0.1.1",
    "cookie-parser": "1.4.3",
    "cors": "2.8.1",
    "debug": "^2.4.5",
    "decamelize": "^1.2.0",
    "dotenv": "^4.0.0",
    "dotenv-expand": "4.0.1",
    "express": "4.14.0",
    "express-flash": "0.0.2",
    "express-session": "1.15.3",
    "express-validation": "1.0.1",
    "express-winston": "2.1.2",
    "file-saver": "^1.3.3",
    "helmet": "3.1.0",
    "http-status": "^0.2.0",
    "joi": "10.0.6",
    "jquery": "3.2.1",
    "lodash.snakecase": "4.1.1",
    "method-override": "^2.3.5",
    "mongoose": "4.11.0",
    "morgan": "1.7.0",
    "multer": "1.3.0",
    "node-forge": "0.7.1",
    "node-sass": "4.5.3",
    "oauth2orize": "1.8.0",
    "passport": "0.3.2",
    "passport-http": "0.3.0",
    "passport-http-bearer": "1.0.1",
    "passport-local": "1.0.0",
    "passport-oauth2-client-password": "0.1.2",
    "pug": "2.0.0-rc.1",
    "sass-loader": "6.0.5",
    "scrypt-async": "1.3.1",
    "semantic-ui-css": "2.2.10",
    "typeface-khula": "0.0.28",
    "typeface-roboto": "0.0.29",
    "uuid": "3.0.1",
    "winston": "2.3.0"
  },
  "devDependencies": {
    "axios": "^0.16.1",
    "babel-cli": "6.18.0",
    "babel-core": "6.18.0",
    "babel-loader": "7.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-env": "1.5.1",
    "babel-preset-stage-2": "6.18.0",
    "chai": "^3.4.1",
    "cheerio": "0.22.0",
    "codeclimate-test-reporter": "0.4.1",
    "commitizen": "^2.9.2",
    "coveralls": "^2.11.6",
    "cross-env": "3.1.3",
    "css-loader": "0.28.2",
    "cz-conventional-changelog": "1.2.0",
    "del": "^2.2.0",
    "eslint": "3.16.1",
    "eslint-config-airbnb-base": "7.1.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-watch": "2.1.14",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-load-plugins": "^1.2.0",
    "gulp-newer": "^1.1.0",
    "gulp-nodemon": "^2.0.6",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "husky": "^0.13.1",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "3.2.0",
    "run-sequence": "^1.1.5",
    "supertest": "2.0.1",
    "supertest-as-promised": "4.0.2",
    "url-loader": "0.5.8",
    "validate-commit-msg": "^2.6.1",
    "webpack": "2.6.0",
    "webpack-stream": "3.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      "env",
      "stage-2"
    ],
    "plugins": [
      "add-module-exports",
      "array-includes"
    ]
  }
}
